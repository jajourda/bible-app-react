{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "env": { "es6": true, "browser": true, "node": true },
  "rules": {
    "prettier/prettier": "error",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-console": 0
  },
  "parser": "babel-eslint"
}
